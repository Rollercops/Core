#include "Sock.h"
#include "libs.h"
#include "fstream"
#include "logging.h"

int	main()
{
  Sock		testSocket(SOCK_STREAM);
  const char	*hostname = "www.error500.fr";

  testSocket.connectByHostname(hostname);
  Logger::destroyAllLogger();
  // IMPORTANT: use Level::destroyAllLogger() to clean.
  return (0);
}
